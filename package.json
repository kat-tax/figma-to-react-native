  "devDependencies": {
    "@create-figma-plugin/build": "^2.5.0",
    "@create-figma-plugin/tsconfig": "^2.5.0",
    "@figma/plugin-typings": "1.68.0",
    "@types/react": "^18.2.13",
    "esbuild": "^0.16.10",
    "react-zoom-pan-pinch": "^3.0.8",
    "typescript": "^4.9.5",
    "typescript-json-schema": "^0.57.0"
